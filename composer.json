{
  "name": "mamikon/swoole-psr7",
  "type": "library",
  "description": "PSR7 converter for Swoole",
  "keywords": [
    "request",
    "response",
    "swoole request",
    "psr7"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Mamikon Arakelyan",
      "email": "m.araqelyan@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.0.0",
    "ext-swoole": "*",
    "psr/http-message": "^1.0",
    "nyholm/psr7": "^1.4",
    "dflydev/fig-cookies": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9"
  },
  "autoload": {
    "psr-4": {
      "Mamikon\\SwoolePsr7\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mamikon\\SwoolePsr7\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
